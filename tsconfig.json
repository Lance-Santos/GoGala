{
    "compilerOptions": {
        "allowJs": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "strict": true,
        "isolatedModules": true,
        "target": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "noEmit": true,
        "paths": {
            "@/*": [
                "./resources/js/*"
            ],
            "ziggy-js": [
                "./vendor/tightenco/ziggy"
            ]
        }
    },
    "include": [
        "resources/ts/**/*.ts",
        "resources/ts/**/*.tsx",
        "resources/ts/**/*.d.ts"
, "resources/js/components/app-sidebar.tsx", "resources/js/components/nav-main.tsx"    ]
}
